<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:bottom="1dp"
        android:top="1dp">
        <shape android:shape="rectangle">
            <corners android:radius="30dp" />
            <stroke
                android:width="0.5dp"
                android:color="#e4ecf4" />
            <padding
                android:bottom="5dp"
                android:left="10dp"
                android:right="10dp"
                android:top="5dp" />

            <solid android:color="#e4ecf4" />
        </shape>
    </item>

</layer-list>
